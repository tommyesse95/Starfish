<!--
M moves to a point, this is optional
L makes a line to a given (x,y) position
l makes a line to a point given by (dx,dy) offsets
-->

<boundaries units="m">
<transform reverse="true" />
<boundary name="inlet" type="virtual">
<path> M 0.0 0.0 L 0.0 2e-3
</path>
</boundary>

<boundary name="wall1" type="solid" material="al" value="0">
<path>
M 0.0 4e-3, 2e-3 4e-3
</path>
</boundary>

<boundary name="keeper" type="solid" material="al" value="20">
<path>
M wall1:last l 0 4e-3
</path>
</boundary>

<boundary name="wall2" type="solid" material="al" value="0">
<path>
M keeper:last L 2e-3 15e-3
</path>
</boundary>


<boundary name="wall3" type="solid" material="al" value="0">
<path>
M wall2:last l 12e-3 0
</path>
</boundary>

<boundary name="neutral_inlet" type="virtual">
<path> M 14e-3 15e-3 L 16e-3 15e-3
</path>
</boundary>

<boundary name="wall4" type="solid" material="al" value="0">
<path>
M 18e-3 15e-3 l 12e-3 0.0
</path>
</boundary>


<boundary name="anode" type="solid" material="al" value="400">
<path>
M wall4:last l 8e-3 0.0
</path>
</boundary>

<boundary name="check" type="sink">
<path>
M  45e-3 15e-3 L 45e-3 0.0
</path>
</boundary>

<boundary name="wall5" type="solid" material="al" value="0">
<path>
M anode:last l 50e-3 0.0
</path>
</boundary>

<boundary name="cathode" type="solid" material="al" value="-400">
<path>
M wall5:last l 8e-3 -8e-3
</path>
</boundary>

<boundary name="wall6" type="solid" material="al" value="10">
<path>
M cathode:last l 25e-3 0.0
</path>
</boundary>

<boundary name="anode2" type="solid" material="al" value="50">
<path>
M wall6:last l 8e-3 0.0
</path>
</boundary>

<boundary name="wall7" type="solid" material="al" value="0">
<path>
M anode2:last l 5e-3 0.0
</path>
</boundary>



</boundaries>
