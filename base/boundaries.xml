<boundary name="wall6" type="solid" material="al" value="0">
<path>
M 0.0 4e-3, 2e-3 4e-3, 2e-3 15e-3, 12e-3 15e-3 <!-- Modify the last value if modified neutral inlet  -->
</path>
</boundary>

<boundary name="neutral_inlet" type="virtual">
<path> M 13e-3 15e-3 L 15e-3 15e-3
</path>
</boundary>

<boundary name="wall1" type="solid" material="al" value="0">
<path>
M 16e-3 15e-3 l 14e-3 0.0
</path>
</boundary>


<boundary name="anode" type="solid" material="al" value="200">
<path>
M wall1:last l 8e-3 0.0
</path>
</boundary>

<boundary name="check" type="sink">
<path>
M  45e-3 15e-3 L 45e-3 0.0
</path>
</boundary>

<boundary name="wall2" type="solid" material="al" value="0">
<path>
M anode:last l 50e-3 0.0
</path>
</boundary>

<boundary name="cathode" type="solid" material="al" value="-200">
<path>
M wall2:last l 8e-3 -8e-3
</path>
</boundary>

<boundary name="wall3" type="solid" material="al" value="10">
<path>
M cathode:last l 25e-3 0.0
</path>
</boundary>

<boundary name="anode2" type="solid" material="al" value="50">
<path>
M wall3:last l 8e-3 0.0
</path>
</boundary>

<boundary name="wall4" type="solid" material="al" value="0">
<path>
M anode2:last l 5e-3 0.0
</path>
</boundary>




</boundaries>
