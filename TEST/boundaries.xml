<!--
M moves to a point, this is optional
L makes a line to a given (x,y) position
l makes a line to a point given by (dx,dy) offsets
-->

<boundaries>



<boundary name="wall2" type="solid" material="al" value="100">
<path>
M 0, 0 l  0 1.5e-2
</path>
</boundary>



<boundary name="wall3" type="open" material="al" value="0">
<path>
M wall2:last l 8e-3 0
</path>
</boundary>


<boundary name="wall4" type="open" material="al" value="0">
<path>
M wall3:last l 8e-3 0
</path>
</boundary>



<boundary name="wall5" type="open" material="al" value="0">
<path>
M wall4:last l 8e-3 0
</path>
</boundary>




<boundary name="wall6" type="solid" material="al" value="-100">
<path>
M wall5:last l 0 -1.5e-2
</path>
</boundary>






</boundaries>
